<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asd.com.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="design_width"
            android:value="768"></meta-data>
        <meta-data
            android:name="design_height"
            android:value="1280"></meta-data>

        <!--<activity android:name=".Activity_retrofit">-->
        <!--<activity android:name=".Activity1">-->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StaggeredGridActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".StaggeredGridActivityFragment" />
        <activity android:name=".StaggeredGridEmptyViewActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".Activity8_eventbus" />
    </application>

</manifest>